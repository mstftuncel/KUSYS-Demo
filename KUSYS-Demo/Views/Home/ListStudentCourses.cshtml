  <!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">Dashboard</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">ListStudentCourses</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            @if (!string.IsNullOrEmpty(ViewBag.Message))
            {
                <div class="alert @(ViewBag.MessageType ?? "alert-info")" role="alert">
                    @ViewBag.Message
                </div>
                <script>
                    setTimeout(function () {
                        $('.alert').slideUp();
                    }, 2000);
                </script>
            }
            <table class="table">
                <thead>
                    <tr>
                        <th>Student</th>
                        <th>Course</th>
                        <th>Description</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var sc in Model)
                    {
                        <tr>
                            <td>@sc.Student.FirstName @sc.Student.LastName</td>
                            <td>@sc.Course.Name</td>
                            <td>@sc.Course.Description</td>
                        </tr>
                    }
                </tbody>
            </table>

        </div><!-- /.container-fluid -->
    </section>
<!-- /.content -->
</div>
<!-- /.content-wrapper -->